{
  "name": "MarkUp Extension",
  "key": "",
  "description": "Base Level Extension",
  "permissions": [
    "http://localhost/*",
    "https://markup-extension.herokuapp.com/*",
    "https://d735b5635bef.ngrok.io/*",
    "storage",
    "tabs",
    "identity"
  ],
  "version": "0.0.0.1",
  "background": {
    "scripts": ["bundledBackground.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/mark_up16.png",
    "48": "images/mark_up48.png",
    "128": "images/mark_up128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/mark_up16.png",
      "48": "images/mark_up48.png",
      "128": "images/mark_up128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["bundledContentScripts.js"]
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens MarkUp Extension"
    }
  }
}
